{
  "key": "$API_KEY$",
  "template_name": "esim-qr-code",
  "template_content": [
    {
      "name": "customerName",
      "content": "$RECEIVER_NAME$"
    }
  ],
  "message": {
    "to": [
      {
        "email": "$RECEIVER_EMAIL$",
        "name": "$RECEIVER_NAME$",
        "type": "to"
      }
    ],
    "important": true,
    "track_opens": true,
    "track_clicks": true,
    "tags": [
      "esim-qr-code"
    ],
    "images": [
      {
        "type": "image/png",
        "name": "eSIM QR code",
        "content": "$QR_CODE$"
      }
    ]
  },
  "async": false
}